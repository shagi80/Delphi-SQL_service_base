  object SumQuery: TADOQuery
    Connection = DMod.Connection
    Parameters = <>
    SQL.Strings = (
      'DECLARE @CNT INT'
      'SET @CNT=0'
      ''
      'SELECT @CNT=COUNT(REC.[ID]) FROM [DBO].[SERVRECORDS] AS REC'
      #9'INNER JOIN [DBO].[SERVREPORT] AS REP ON REP.[ID]=REC.[REPORTID]'
      
        #9'INNER JOIN [DBO].[SERVCENTRES] AS SNT ON SNT.[ID]=REP.[SENTERID' +
        ']'
      'WHERE (REC.[STATUS]=5) AND (REC.[ISADDPART]=0) /FILTER/'
      ''
      
        'SELECT @CNT AS CNT, SUM(coalesce(REC.[WORKPRICE],0)) AS TWORK, S' +
        'UM(coalesce(REC.[MOVPRICE],0)) AS TMOV,'
      
        #9'SUM(coalesce(REC.[PARTCOST],0)) AS TPART, (SUM(coalesce(REC.[WO' +
        'RKPRICE],0))+SUM(coalesce(REC.[MOVPRICE],0))+SUM(coalesce(REC.[P' +
        'ARTCOST],0))) AS TOTAL'
      #9'FROM [DBO].[SERVRECORDS] AS REC'
      #9'INNER JOIN [DBO].[SERVREPORT] AS REP ON REP.[ID]=REC.[REPORTID]'
      
        #9'INNER JOIN [DBO].[SERVCENTRES] AS SNT ON SNT.[ID]=REP.[SENTERID' +
        ']'
      'WHERE (REC.[STATUS]=5) /FILTER/'
      ''
      '')
    Left = 552
    Top = 8
  end
