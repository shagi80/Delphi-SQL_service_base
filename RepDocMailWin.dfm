  object MainQuery: TADOQuery
    Connection = DMod.Connection
    Parameters = <>
    SQL.Strings = (
      
        'SELECT FORMAT(REP.[DOCMAILDATE],'#39'dd.MM.yy'#39') AS MAILDATE, FORMAT(' +
        'REP.[REPDATE],'#39'MMMM yyyy'#39') AS REPDATE, REP.[NOTE],'
      
        #9'SNT.[CITY], SNT.[DESCR], SNT.[CODE], SNT.[EMPLID], SNT.[REGIONI' +
        'D],'
      
        #9'(SELECT COUNT(REC.[ID]) FROM [DBO].[SERVRECORDS] AS REC WHERE R' +
        'EC.[REPORTID]=REP.[ID] AND REC.[STATUS]=5 AND REC.[ISADDPART]=0)' +
        ' AS RECCNT,'
      
        #9'(SELECT SUM(REC.[PARTCOST]) FROM [DBO].[SERVRECORDS] AS REC WHE' +
        'RE REC.[REPORTID]=REP.[ID] AND REC.[STATUS]=5) AS PARTSUM,'
      
        #9'(SELECT SUM(REC.[MOVPRICE]) FROM [DBO].[SERVRECORDS] AS REC WHE' +
        'RE REC.[REPORTID]=REP.[ID] AND REC.[STATUS]=5) AS MOVSUM,'
      
        #9'(SELECT SUM(REC.[WORKPRICE]) FROM [DBO].[SERVRECORDS] AS REC WH' +
        'ERE REC.[REPORTID]=REP.[ID] AND REC.[STATUS]=5) AS WORKPRICE,'
      
        #9'(SELECT SUM(coalesce(REC.[PARTCOST],0)+coalesce(REC.[WORKPRICE]' +
        ',0)+coalesce(REC.[MOVPRICE],0))'
      
        #9#9#9'FROM [DBO].[SERVRECORDS] AS REC WHERE REC.[REPORTID]=REP.[ID]' +
        ' AND REC.[STATUS]=5) AS TOTALSUM,'
      #9'REP.[FIRSTINVOICENUMBER], REP.[FIRSTINVOICEDATE]'
      
        'FROM [DBO].[SERVREPORT] AS REP INNER JOIN [DBO].[SERVCENTRES] AS' +
        ' SNT ON REP.[SENTERID]=SNT.[ID]'
      'WHERE /FILTER/'
      'ORDER BY MAILDATE')
    Left = 456
    Top = 8
  end
  object DS: TDataSource
    DataSet = MainQuery
    Left = 496
    Top = 8
  end
  object SumQuery: TADOQuery
    Connection = DMod.Connection
    Parameters = <>
    SQL.Strings = (
      'SELECT'
      '  FORMAT(SUM(REC.[MOVPRICE]),'#39'## ### ##0.##'#39') AS TMOV,'
      '  FORMAT(SUM(REC.[PARTCOST]),'#39'## ### ##0.##'#39') AS TPART,'
      '  FORMAT(SUM(REC.[WORKPRICE]),'#39'## ### ##0.##'#39') AS TWORK,'
      
        #9'FORMAT(SUM(coalesce(REC.[PARTCOST],0)+coalesce(REC.[WORKPRICE],' +
        '0)+coalesce(REC.[MOVPRICE],0)),'#39'## ### ##0.##'#39' ) AS TOTAL,'
      #9'(SELECT COUNT(REP.[ID]) FROM [DBO].[SERVREPORT] AS REP'
      
        '    INNER JOIN [DBO].[SERVCENTRES] AS SNT ON REP.[SENTERID]=SNT.' +
        '[ID] WHERE /FILTER/) AS CNT'
      
        'FROM [DBO].[SERVRECORDS] AS REC INNER JOIN [DBO].[SERVREPORT] AS' +
        ' REP ON REC.[REPORTID]=REP.[ID]'
      
        #9'INNER JOIN [DBO].[SERVCENTRES] AS SNT ON REP.[SENTERID]=SNT.[ID' +
        ']'
      'WHERE REC.[STATUS]=5 AND /FILTER/'
      '')
    Left = 552
    Top = 8
  end
  object SumDS: TDataSource
    DataSet = SumQuery
    Left = 592
    Top = 8
  end
