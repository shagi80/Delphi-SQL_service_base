  object SumQuery: TADOQuery
    Connection = DMod.Connection
    Parameters = <>
    SQL.Strings = (
      'DECLARE @CNT INT'
      'SET @CNT=0'
      ''
      'SELECT @CNT=COUNT(REC.[ID]) FROM [DBO].[SERVRECORDS] AS REC'
      #9'INNER JOIN [DBO].[SERVREPORT] AS REP ON REP.[ID]=REC.[REPORTID]'
      
        #9'INNER JOIN [DBO].[SERVCENTRES] AS SNT ON SNT.[ID]=REP.[SENTERID' +
        ']'
      'WHERE (REC.[ISADDPART]=0) AND REC.[ACCEPTDATE] /FILTER/'
      ''
      'SELECT'
      #9'COUNT(DISTINCT REP.[ID]) AS CNT,'
      #9'@CNT AS RECCNT,'
      '  FORMAT(SUM(REC.[PARTCOST]),'#39'### ### ###.##'#39') AS PARTSUM,'
      '  FORMAT(SUM(REC.[MOVPRICE]),'#39'### ### ###.##'#39') AS MOVSUM,'
      '  FORMAT(SUM(REC.[WORKPRICE]),'#39'### ### ###.##'#39') AS WORKSUM,'
      
        #9'FORMAT(SUM(coalesce(REC.[PARTCOST],0)+coalesce(REC.[WORKPRICE],' +
        '0)+coalesce(REC.[MOVPRICE],0)),'#39'### ### ###.##'#39') AS TOTALSUM'
      #9'FROM [SERVRECORDS] AS REC'
      #9'LEFT JOIN [SERVREPORT] AS REP ON REP.[ID]=REC.[REPORTID]'
      #9'WHERE REC.[ACCEPTDATE] /FILTER/'
      '')
    Left = 552
    Top = 8
  end
